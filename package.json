{
  "name": "usb-spy",
  "version": "0.1.0",
  "description": "监听USB接口，设置USB白名单，弹出USB设备，区分USB设备类型",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "chcp 65001 && rimraf dist && vite",
    "build": "rimraf dist && vite build",
    "fix": "chcp 65001 && electron-fix start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:main && npm run test:render",
    "test:render": "vitest run -r src/render --passWithNoTests",
    "test:main": "vitest run -r src/main --passWithNoTests",
    "test:e2e": "vitest run",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder uninstall-app-deps",
    "rebuild": "electron-rebuild"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "crypto-ts": "^1.0.2",
    "einf": "^1.3.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "ffi-napi": "^4.0.3",
    "jsonfile": "^6.1.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "node-powershell-updates": "^4.0.1",
    "ref-array-napi": "^1.2.1",
    "ref-napi": "^3.0.1",
    "ref-struct-napi": "^1.1.1",
    "systeminformation": "^5.17.9",
    "usb-detection": "^4.14.1",
    "vue": "^3.2.38",
    "winreg-ts": "^1.0.4"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "0.0.7",
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-vue": "3.1.0",
    "@vue/compiler-sfc": "3.2.39",
    "@vue/test-utils": "2.0.2",
    "electron": "20.0.0",
    "electron-builder": "23.3.3",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-fix": "^1.1.3",
    "electron-rebuild": "^3.2.9",
    "eslint": "8.22.0",
    "happy-dom": "6.0.4",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "13.0.3",
    "naive-ui": "^2.34.3",
    "parse-json": "^6.0.2",
    "playwright": "1.26.0",
    "rimraf": "3.0.2",
    "simple-git-hooks": "2.8.0",
    "tslib": "2.4.0",
    "typescript": "4.8.3",
    "vite": "3.1.3",
    "vite-plugin-doubleshot": "0.0.6",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "0.23.4",
    "vue-router": "~4.0.x",
    "vue-tsc": "0.40.13"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
